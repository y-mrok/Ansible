.highlight-none pre {
    background: whitesmoke;
    color: #191970;
}

.wy-nav-content {
  max-width: 1055px;
}

.wy-table-responsive table td, .wy-table-responsive table th {
    white-space: normal;
}

h1 {
  color: #191970;/*文字色*/
  padding: 0.2em 0;/*上下の余白*/
  border-top: solid 5px #191970;/*上線*/
  border-bottom: solid 5px #191970;/*下線*/
}

h2 {
  color: #191970;/*文字色*/
  padding: 0.2em 0;/*上下の余白*/
  border-bottom: solid 3px #191970;/*上線*/
}

h3 {
  color: #191970;/*文字色*/
  padding: 0.2em 0;/*上下の余白*/
  border-bottom: solid 1px #191970;/*上線*/
}

h4 {
  position: relative;
  padding-left: 1.2em;
  line-height: 1.4;
}
h4:before {
  font-family: FontAwesome;
  content: "\f00c";
  font-weight: 400;
  position: absolute;
  font-size: 1em;
  left: 0;
  top: 0;
  color: #191970;
}

h5 {
    position: relative;
    padding-left: 1.2em;
    line-height: 1.4;
  }
  h5:before {
    font-family: FontAwesome;
    content: "\f054";
    font-weight: 400;
    position: absolute;
    font-size: 1em;
    left: 0;
    top: 0;
    color: #191970;
  }

h6 {
    position: relative;
    padding-left: 1.2em;
    line-height: 1.4;
  }
  
  h6:before {
    font-family: FontAwesome;
    content: "\f0da";
    font-weight: 400;
    position: absolute;
    font-size: 1em;
    left: 0;
    top: 0;
    color: #191970; 
  }